{
  "name": "tiny-context",
  "description": "A tiny library for context using react-hooks.",
  "keywords": [
    "tiny-context",
    "react",
    "hooks",
    "context",
    "store"
  ],
  "version": "1.2.1",
  "license": "MIT",
  "author": {
    "name": "Akito Moriki",
    "email": "benishouga@gmail.com",
    "url": "https://github.com/benishouga/"
  },
  "main": "lib/tiny-context.js",
  "unpkg": "umd/tiny-context.js",
  "module": "es/tiny-context.js",
  "types": "lib/tiny-context.d.ts",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/benishouga/tiny-context.git"
  },
  "bugs": "https://github.com/benishouga/tiny-context/issues",
  "homepage": "https://github.com/benishouga/tiny-context",
  "scripts": {
    "clean": "rimraf coverage es lib umd",
    "prepare": "npm run clean && npm run compile && npm run lint && npm run test && npm run build",
    "compile": "tsc --noEmit",
    "lint": "eslint --ext=tsx src test",
    "build": "rollup -c",
    "start": "webpack-dev-server --hot --watch-content-base --content-base docs/ --open ",
    "docs": "webpack --mode=development",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "format": "prettier --write \"./src/**/*.*\" \"./test/**/*.*\" \"./**/*.json\""
  },
  "peerDependencies": {
    "react": "^16.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@types/jest": "25.1.2",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "@types/react-syntax-highlighter": "11.0.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "raw-loader": "4.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-syntax-highlighter": "12.2.1",
    "rimraf": "3.0.2",
    "rollup": "1.31.1",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-typescript2": "0.26.0",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
